@{
    Layout = null;
}
<html>
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width">
    <title>统一错误页</title>
    <style>
        body {
            font-size: 14px;
            font-family: 'helvetica neue',tahoma,arial,'hiragino sans gb','microsoft yahei','Simsun',sans-serif;
            background-color: #fff;
            color: #808080;
        }

        .wrap {
            margin: 200px auto;
            width: 510px;
        }

        td {
            text-align: left;
            padding: 2px 10px;
        }

            td.header {
                font-size: 22px;
                padding-bottom: 10px;
                color: #000;
            }

            td.check-info {
                padding-top: 20px;
            }

        a {
            color: #328ce5;
            text-decoration: none;
        }

            a:hover {
                text-decoration: underline;
            }
    </style>
</head>
<body>
    <div class="wrap">
        <table>
            <tr>
                <td rowspan="5" style=""><img src="@Url.Content("~/QWebFramework/Images/error.jpg")"/></td>
                <td class="header">很抱歉，发生错误！</td>
            </tr>
            <tr><td>原因：@ViewData["Message"]</td></tr>
            <tr><td><a href="@Url.Content("~/Login.html")">重新登录</a></td></tr>
        </table>
    </div>
</body>
</html>  